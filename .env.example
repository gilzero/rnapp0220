EXPO_PUBLIC_ENV="DEVELOPMENT"

# Your development URL (localhost or ngrok)
EXPO_PUBLIC_DEV_API_URL="http://localhost:3050"

# Your production URL
EXPO_PUBLIC_PROD_API_URL="https://your-production-api.com"

# Message Validation (must match server configuration)
EXPO_PUBLIC_MAX_MESSAGE_LENGTH=24000
EXPO_PUBLIC_MAX_MESSAGES_IN_CONTEXT=50
EXPO_PUBLIC_MIN_MESSAGE_LENGTH=1
EXPO_PUBLIC_MAX_INPUT_LENGTH=18000

# Network Configuration
EXPO_PUBLIC_API_REQUEST_TIMEOUT=30000
EXPO_PUBLIC_STREAM_TIMEOUT=60000
EXPO_PUBLIC_CONNECTION_TIMEOUT=10000
EXPO_PUBLIC_SOCKET_TIMEOUT=5000

# Retry Configuration
EXPO_PUBLIC_MAX_RETRY_ATTEMPTS=3
EXPO_PUBLIC_RETRY_BACKOFF_MS=1000
EXPO_PUBLIC_MAX_RETRY_BACKOFF_MS=5000

# Cache Configuration
EXPO_PUBLIC_MESSAGE_TTL=86400000
EXPO_PUBLIC_MAX_CACHE_SIZE=52428800
EXPO_PUBLIC_CACHE_INVALIDATION_INTERVAL=3600000


# DEFAULT_MODEL_PARAMS
EXPO_PUBLIC_TEMPERATURE=0.3
EXPO_PUBLIC_MAX_TOKENS=4096


# Rate Limits Configuration
EXPO_PUBLIC_REQUESTS_PER_MINUTE=60
EXPO_PUBLIC_CONCURRENT_STREAMS=3